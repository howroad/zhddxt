package com.project.dto;

import java.io.Serializable;

/**
 * 突发事件表格数据
 * 
 * @author 赵子墨
 *
 */
@SuppressWarnings("serial")
public class ReportTableDto implements Serializable {
	private String RId;
	private String RCode;
	private String RName;
	private String RRegion;
	private String RGrade;
	private String RType;
	private String RDate;
	private Integer RStatus;

	public ReportTableDto() {
	}

	public ReportTableDto(String rId, String rCode, String rName, String rRegion, String rGrade, String rType, String rDate,
			Integer rStatus) {
		super();
		RId = rId;
		RCode = rCode;
		RName = rName;
		RRegion = rRegion;
		RGrade = rGrade;
		RType = rType;
		RDate = rDate;
		RStatus = rStatus;
	}

	public String getRId() {
		return RId;
	}

	public void setRId(String rId) {
		RId = rId;
	}

	public String getRCode() {
		return RCode;
	}

	public void setRCode(String rCode) {
		RCode = rCode;
	}

	public String getRName() {
		return RName;
	}

	public void setRName(String rName) {
		RName = rName;
	}

	public String getRRegion() {
		return RRegion;
	}

	public void setRRegion(String rRegion) {
		RRegion = rRegion;
	}

	public String getRGrade() {
		return RGrade;
	}

	public void setRGrade(String rGrade) {
		RGrade = rGrade;
	}

	public String getRType() {
		return RType;
	}

	public void setRType(String rType) {
		RType = rType;
	}

	public String getRDate() {
		return RDate;
	}

	public void setRDate(String rDate) {
		RDate = rDate;
	}

	public Integer getRStatus() {
		return RStatus;
	}

	public void setRStatus(Integer rStatus) {
		RStatus = rStatus;
	}

	@Override
	public String toString() {
		return "ReportTableDto [RId=" + RId + ", RCode=" + RCode + ", RName=" + RName + ", RRegion=" + RRegion + ", RGrade="
				+ RGrade + ", RType=" + RType + ", RDate=" + RDate + ", RStatus=" + RStatus + "]";
	}

}
